{
  "designerVersion" : "1.9.53",
  "id" : "customNavegador",
  "name" : "Navegador",
  "lastUpdate" : 1591125707990,
  "template" : "<form id=\"radioButtonForm\" name=\"radioButtonForm\">\n<md-radio-group >\n    <div>\n        <input type=\"radio\" name=\"group\" value=\"lastDirectory\" class=\"md-primary\" ng-click=\"ctrl.cambiarValor('anterior')\">\n        <label for=\"lastDirectory\">Volver a la categor√≠a anterior</label>\n    </div>\n    <div ng-repeat=\"x in properties.listaDirectorios\">\n        <input type=\"radio\"  id=\"{{ x }}\" name=\"group\" value=\"{{ x }}\" ng-click=\"ctrl.cambiarValor(x)\">\n        <label for=\"{{ x }}\">{{x}}</label>\n    </div>\n</md-radio-group>\n</form>\n",
  "controller" : "\nfunction ($scope) {\n    \n    this.cambiarValor = function(cat){\n        if(cat != \"anterior\"){\n            alert(\"Entrando\");\n            $scope.properties.nuevoDir = $scope.properties.directorioActual + \"/\" + cat; \n        }else{\n            alert(\"Regresando\");\n            $scope.properties.nuevoDir = $scope.properties.directorioAnterior;\n        }\n        \n    };\n}",
  "custom" : true,
  "properties" : [ {
    "label" : "Directorio actual",
    "name" : "directorioActual",
    "type" : "text",
    "bond" : "variable"
  }, {
    "label" : "Directorios de la carpeta actual",
    "name" : "listaDirectorios",
    "type" : "text",
    "bond" : "variable"
  }, {
    "label" : "Directorio anterior",
    "name" : "directorioAnterior",
    "type" : "text",
    "bond" : "variable"
  }, {
    "label" : "Nuevo directorio",
    "name" : "nuevoDir",
    "type" : "text",
    "bond" : "variable"
  } ],
  "assets" : [ ],
  "requiredModules" : [ ],
  "type" : "widget",
  "hasHelp" : false
}